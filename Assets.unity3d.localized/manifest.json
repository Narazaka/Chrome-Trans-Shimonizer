{
  "manifest_version": 2,
  "name": "ﾍﾟーｼﾞーｼﾓﾅｲｻﾞー",
  "description": "ｼﾓﾅｲｽﾞ",
  "version": "1.0.0",
  "default_locale": "ja",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons" : {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
	  "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "lib/jquery.slim.min.js",
        "js/mid.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
